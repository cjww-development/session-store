# Routes
# This file defines all application app.routes (Higher priority app.routes first)
# ~~~~

POST    /session/:sessionId/cache               controllers.SessionController.cache(sessionId: String)
GET     /session/:sessionId/data/:key           controllers.SessionController.getEntry(sessionId: String, key: String)
PUT     /session/:sessionId                     controllers.SessionController.updateSession(sessionId: String)
DELETE  /session/:sessionId/destroy             controllers.SessionController.destroy(sessionId: String)
